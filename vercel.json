{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node",
            "config": {
                "maxLambdaSize": "15mb",
                "includeFiles": [
                    "cookies.txt",
                    "scripts/**",
                    "playground.html",
                    "playground.js"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.js"
        }
    ]
}