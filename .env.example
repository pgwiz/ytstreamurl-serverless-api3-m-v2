# DigitalOcean Serverless Function - Environment Variables Template

# Python Environment
PYTHONUNBUFFERED=1

# yt-dlp Configuration
YT_DLP_PATH=/usr/local/bin/yt-dlp
LOG_DIR=/tmp/proxyLogs
REQUEST_TIMEOUT=45
COOKIES_FILE=/tmp/cookies.txt

# Flask Configuration
FLASK_ENV=production
FLASK_APP=serverless_handler.py
PORT=8000

# DigitalOcean Configuration
DIGITALOCEAN_ACCESS_TOKEN=dop_v1_your_token_here

# Local Development (optional)
DEBUG=false
